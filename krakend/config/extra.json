{
    "security/cors": {
      "allow_origins": [
        "*"
      ],
      "allow_methods": [
        "GET",
        "POST",
        "DELETE",
        "PUT",
        "PATCH",
        "OPTIONS",
        "HEAD"
      ],
      "expose_headers": [
        "Content-Length",
        "Content-Type"
      ],
      "allow_headers": [
        "Authorization",
        "Accept-Language",
        "Content-Type",
        "X-Api-Key"
      ],
      "debug": false
    }
}
  